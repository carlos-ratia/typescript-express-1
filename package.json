{
  "name": "typescript-express-1",
  "version": "1.0.0",
  "description": "Node + ExpressJs + Typescript",
  "main": "index.js",
  "scripts": {
    "build": "npm run db:migration:up && npm run prisma:introspect &&  npm run prisma:validate && npm run prisma:generate && npm run build:ts",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "db:migration:up": "npx sequelize-cli db:migrate",
    "db:migration:down": "npx sequelize-cli db:migrate:undo",
    "db:migration:create": "npx sequelize-cli  migration:generate --migrations-path=src/Migrations",
    "watch:serve": "nodemon src/index.ts",
    "prisma:introspect": "npx prisma introspect",
    "prisma:generate": "npx prisma generate",
    "prisma:validate": "npx prisma validate",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlos-ratia/typescript-express-1.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carlos-ratia/typescript-express-1/issues"
  },
  "homepage": "https://github.com/carlos-ratia/typescript-express-1#readme",
  "devDependencies": {
    "@prisma/cli": "^2.15.0",
    "@types/bluebird": "^3.5.33",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/lodash": "^4.14.168",
    "@types/sequelize": "^4.28.9",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@prisma/client": "^2.15.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0"
  }
}
